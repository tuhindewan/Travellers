	.drop_target::before {
	    content: 'Drop Hear';
	    font-family: FontAwesome;
	    position: absolute;
	    display: block;
	    width: 100%;
	    line-height: 134px;
	    text-align: center;
	    font-size: 20px;
	    color: rgba(0, 0, 0, 0.3);
	    transition: color 0.2s;
	}

	.dl-menuwrapper button {
	    background: transparent;
	    border: none;
	    padding: 0 10px;
	    width: 100%;
	    height: 30px;
	    overflow: hidden;
	    position: relative;
	    cursor: pointer;
	    outline: none;
	    color: #fff;
	    border-radius: 5px;
	    margin: 5px 0px;
	    font-size: 10px;
	}

	.dl-menuwrapper .dl-menu {
        margin: 0;
        position: absolute;
        width: 100%;
        opacity: 0;
        pointer-events: none;
        -webkit-transform: translateY(10px);
        -moz-transform: translateY(10px);
        transform: translateY(10px);
        -webkit-backface-visibility: hidden;
        -moz-backface-visibility: hidden;
        backface-visibility: hidden;
        top: 37px;
        height: 30px;
	}


	.dl-menuwrapper img{
		width: 22px !important;
		height: auto !important;
		border-radius: 50%;
		margin-right: 5px;
		border: 1px solid #737a88;
		padding: 3px;
	}

	.dl-menuwrapper li a {
	    display: block;
	    position: relative;
	    padding: 10px 15px;
	    font-size: 10px;
